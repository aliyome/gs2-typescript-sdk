{
  "name": "gs2-typescript-sdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {},
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "typescript": "^3.7.4",
    "tslib": "^1.11.2",
    "fork-ts-checker-webpack-plugin": "^4.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "jest": "^25.5.4",
    "ts-jest": "^25.4.0",
    "babel-jest": "^25.4.0",
    "@types/jest": "^25.2.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "test": "jest",
  "jest": {
    "moduleNameMapper": {
      "@/gs2/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  },
  "type": "module"
}
